BrainShell {
	Expr    = AddExpr 
	
	AddExpr = AddExpr "+" MulExpr --plus
        | AddExpr "-" MulExpr --minus
        | MulExpr
	MulExpr = MulExpr "*" MulExpr --multiply
		| MulExpr "/" MulExpr --divide
		| ExpExpr
	ExpExpr = ExpExpr "^" ExpExpr -- power
	    | PriExpr
	PriExpr = "(" Expr ")" --paren
		| Literal
    Literal = Number Unit?
	Number = hex | float | integer
	hex = "0x" hexDigit+
	float = digit+ "." digit+ exp?
	exp = "e" "-"? digit+
	integer = digit+ exp?
	Unit = letter+
}
